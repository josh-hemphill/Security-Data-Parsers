{
	"dotnet.defaultSolution": "SecurityDataParsers.sln",
	"cSpell.words": [
		"Codecov",
		"FASC",
		"FASCN",
		"keygen",
		"Newtonsoft",
		"Oids",
		"strs",
		"Teletex",
		"Unbrowsable",
		"Xunit"
	],
	"terminal.integrated.automationProfile.windows": {
		"overrideName": "PowerShell"
	},
	"terminal.integrated.defaultProfile.windows": "PowerShell",
	"csharp.format.enable": true,
	"csharp.suppressDotnetRestoreNotification": true,
	"omnisharp.useModernNet": true,
	"omnisharp.useEditorFormattingSettings": true,
	"omnisharp.organizeImportsOnFormat": true,
	"omnisharp.enableEditorConfigSupport": true,
	"[csharp]": {
		"editor.maxTokenizationLineLength": 2500,
		"editor.inlineSuggest.suppressSuggestions": false,
		"editor.defaultFormatter": "ms-dotnettools.csharp"
	},
	"[dotnetProject]": {
		"editor.defaultFormatter": "ms-dotnettools.csharp"
	},
	"editor.tabSize": 3,
	"yaml.schemas": {
		"https://gist.githubusercontent.com/josh-hemphill/0018bf51444072d1cec1eb633a7f0c40/raw/v2.4.1.json": [
			"*caddy*.yaml",
			"*caddy*.yml"
		],
		"https://json.schemastore.org/github-workflow.json": "file:///e%3A/Share/dev/SecurityDataParsers/.github/workflows/codecov.yml"
	}
}
